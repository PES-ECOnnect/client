<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile.SetHomeFragment">

    <EditText
        android:id="@+id/escala_value"
        android:layout_width="164dp"
        android:layout_height="43dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="24dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/floor"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/escala"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="12dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/escala_value"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/save_button"
        android:layout_width="161dp"
        android:layout_height="45dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="44dp"
        android:text="SAVE"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/cancel_button"
        android:layout_width="161dp"
        android:layout_height="45dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="44dp"
        android:text="CANCEL"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/save_button" />

    <EditText
        android:id="@+id/postal_code_num"
        android:layout_width="175dp"
        android:layout_height="44dp"
        android:layout_marginStart="56dp"
        android:layout_marginBottom="20dp"
        android:ems="10"
        android:inputType="textPostalAddress"
        app:layout_constraintBottom_toTopOf="@+id/street"
        app:layout_constraintStart_toEndOf="@+id/set_home_view" />

    <EditText
        android:id="@+id/street_text"
        android:layout_width="185dp"
        android:layout_height="48dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="12dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/num_street"
        app:layout_constraintEnd_toStartOf="@+id/change_street_button" />

    <TextView
        android:id="@+id/num_street"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="136dp"
        android:layout_marginBottom="12dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/num_street_value"
        app:layout_constraintEnd_toStartOf="@+id/escala" />

    <EditText
        android:id="@+id/door_value"
        android:layout_width="164dp"
        android:layout_height="43dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="36dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/check_home_button"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/check_home_button"
        android:layout_width="150dp"
        android:layout_height="48dp"
        android:layout_marginBottom="120dp"
        android:text="VALIDATE"
        android:textColor="#263238"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/floor_num"
        android:layout_width="164dp"
        android:layout_height="43dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="36dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/check_home_button"
        app:layout_constraintEnd_toStartOf="@+id/door_value" />

    <TextView
        android:id="@+id/floor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="140dp"
        android:layout_marginBottom="16dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/floor_num"
        app:layout_constraintEnd_toStartOf="@+id/door" />

    <Button
        android:id="@+id/change_street_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="12dp"
        android:text="SELECT STREET"
        app:layout_constraintBottom_toTopOf="@+id/num_street"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/street"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="324dp"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/street_text"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/door"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="140dp"
        android:layout_marginBottom="24dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/door_value"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/num_street_value"
        android:layout_width="164dp"
        android:layout_height="43dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="24dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/floor"
        app:layout_constraintEnd_toStartOf="@+id/escala_value" />

    <TextView
        android:id="@+id/postal_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="32dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/postal_code_num"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/set_home_view"
        android:layout_width="135dp"
        android:layout_height="37dp"
        android:layout_marginEnd="148dp"
        android:layout_marginBottom="20dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/postal_code_num"
        app:layout_constraintEnd_toStartOf="@+id/postal_code" />

</androidx.constraintlayout.widget.ConstraintLayout>